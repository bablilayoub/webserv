server {
	listen 9000;
	server_names localhost;
	limit_client_body_size 500MB;
	root_folder /Users/alaalalm/Desktop/webserv/Html;
	
	location / {
		autoindex on;
	    accepted_methods GET;
	}
	
	location /delete {
		autoindex off;
		index delete.html;
	    accepted_methods GET;
	}
	
	location /login {
		index login.html;
	    upload_dir /path/to/upload/directory;
	    accepted_methods GET, POST;
	}

	location /video.mp4 {
		index video.mp4;
	    accepted_methods GET;
	}

	location /image.png {
		index image.png;
	    accepted_methods GET;
	}

	# error_page 404 errors/404.html;
	error_page 405 errors/405.html;
	error_page 500 errors/500.html;
}