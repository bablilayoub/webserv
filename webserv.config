# Default server
server {
	listen 9000;
	server_names localhost;
	limit_client_body_size 500MB;

	location / {
		autoindex on;
		root_folder /Users/abablil/Desktop/webserv/Html;
	    upload_dir /path/to/upload/directory;
	    accepted_methods POST, GET;
	}
	
	location /delete {
		index index.html;
		default_file index.html;
		root_folder /Users/abablil/Desktop/webserv/Html;
	    upload_dir /path/to/upload/directory;
	    accepted_methods POST, GET;
	}

	error_page 404 /Users/abablil/Desktop/webserv/Html/404.html;
	error_page 405 /Users/abablil/Desktop/webserv/Html/405.html;
	error_page 500 /Users/abablil/Desktop/webserv/Html/500.html;
}

